<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>Boundless Exchange</title>
    <% if (htmlWebpackPlugin.options.config.optimize) { %>
      <script>if('serviceWorker' in navigator){navigator.serviceWorker.register('/serviceWorker.js',{scope:'/'})}</script>
    <% } %>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
    <% } %>
    <style id="component-styles"></style>
  </head>
  <body>
    <div id="root"></div>
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <script src="<%= htmlWebpackPlugin.files.chunks[chunk].entry %>"></script>
    <% } %>
  </body>
</html>
